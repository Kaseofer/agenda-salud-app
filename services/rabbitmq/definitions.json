{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password": "admin123",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "notifications.events",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "professional.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "notificationservice.notifications",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "notifications.events.dlx",
        "x-message-ttl": 86400000,
        "x-queue-type": "classic"
      }
    },
    {
      "name": "postino_email_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "authservice.professional.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-queue-type": "classic"
      }
    },
    {
      "name": "appointmentservice.professional.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-queue-type": "classic"
      }
    }
  ],
  "bindings": [
    {
      "source": "notifications.events",
      "vhost": "/",
      "destination": "notificationservice.notifications",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    },
    {
      "source": "professional.events",
      "vhost": "/",
      "destination": "appointmentservice.professional.events",
      "destination_type": "queue",
      "routing_key": "professional.*",
      "arguments": {}
    },
    {
      "source": "professional.events",
      "vhost": "/",
      "destination": "authservice.professional.events",
      "destination_type": "queue",
      "routing_key": "professional.*",
      "arguments": {}
    }
  ]
